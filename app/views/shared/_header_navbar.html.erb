        <div class="container">
            <%= link_to root_path, :class => "brand" do %>
                <img src="/assets/kb/logo.png" alt="Det Kongelige Bibliotek" class="visible-phone">
                <img src="/assets/kb/logoDa.png" alt="Det Kongelige Bibliotek" class="hidden-phone">
            <%end%>
            <%#= render :partial=>'/user_util_links' %>
        </div>
    </div>
</header>

